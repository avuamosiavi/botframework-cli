{
    "$schema": "https://raw.githubusercontent.com/microsoft/botbuilder-dotnet/chrimc/map/schemas/sdk.schema",
    "$type": "Microsoft.OnSetProperty",
    "property": "PropertyToChange",
    "actions": [
        {
            "$type": "Microsoft.IfCondition",
            "condition":"empty(where(dialog.requiredProperties, property, !dialog[property]))",
            "actions":[
                {
                    "$type": "Microsoft.SetProperty",
                    "property": "$PropertyToChange",
                    "value": "concat('dialog.',@PROPERTYNAME)"
                },
                {
                    "$type": "Microsoft.DeleteProperty",
                    "property": "{dialog.PropertyToChange}"            
                }
            ]
        }   
    ]
}